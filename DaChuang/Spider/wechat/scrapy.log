09\u793a\u8303\u6027\u5fae\u7535\u5b50\u5b66\u9662\u4e2d\u4e5f\u5c5e\u7f55\u89c1\u3002\u5728\u751f\u6d3b\u4e2d\uff0c\u540c\u5b66\u4e4b\u95f4\u4e92\u5e2e\u4e92\u52a9\uff0c\u73ed\u59d4\u53ca\u65f6\u4e86\u89e3\u5404\u540c\u5b66\u601d\u60f3\u52a8\u5411\uff0c\u6211\u4eec\u8fd8\u4e3e\u529e\u4e86\u5fae\u7535\u5b50\u4e13\u4e1a\u5b66\u957f\u8003\u7814\u4fdd\u7814\u7ecf\u9a8c\u5206\u4eab\u4f1a\uff0c\u8ba9\u5404\u4f4d\u540c\u5b66\u79ef\u6781\u4e86\u89e3\u5f62\u52bf\uff0c\u5c3d\u65e9\u505a\u51fa\u4eba\u751f\u8ba1\u5212\u3002\u515a\u56e2\u5efa\u8bbe\u65b9\u9762\uff0c\u622a\u6b62\u76ee\u524d\uff0c\u6211\u73ed\u5df2\u7ecf\u6709\u4e03\u540d\u540c\u5b66\u5165\u515a\uff0c\u8fd1\u5341\u4f4d\u540c\u5b66\u6210\u4e3a\u5165\u515a\u79ef\u6781\u5206\u5b50\uff0c\u540c\u65f6\uff0c\u56e2\u652f\u4e66\u8c22\u65b0\u6631\u540c\u5fd7\u5728\u7ec4\u7ec7\u56e2\u65e5\u6d3b\u52a8\u65f6\uff0c\u4e5f\u6709\u610f\u8bc6\u5730\u5c06\u65f6\u4ee3\u4e3b\u65cb\u5f8b\u878d\u5165\u8fdb\u6765\uff0c\u8ba9\u6211\u73ed\u540c\u5b66\u53d1\u65f6\u4ee3\u4e4b\u58f0\uff0c\u7b51\u65f6\u4ee3\u4e4b\u68a6\uff01\u5148\u8fdb\u4e8b\u8ff91. \xa0\u5168\u73ed\u5171\u6709\u516b\u4eba\u5728\u5168\u56fd\u5927\u5b66\u751f\u96c6\u6210\u7535\u8def\u521b\u65b0\u521b\u4e1a\u5927\u8d5b\u4e2d\u83b7\u5f97\u5956\u52b1\u3002\u5176\u4e2d\uff0c\u4e00\u7b49\u5956\u4e00\u9879\u3001\u4e8c\u7b49\u5956\u4e00\u9879\u3001\u4e09\u7b49\u5956\u4e00\u9879\u30022. \xa0 \u73ed\u7ea7\u4e0e\u4e1c\u5927\u9547\u5357\u77f3\u6751\u5efa\u7acb\u5408\u4f5c\u5173\u7cfb\uff0c\u73ed\u7ea7\u96c6\u4f53\u5229\u7528\u8bfe\u4f59\u65f6\u95f4\u524d\u5f80\u5357\u77f3\u6751\u5bf9\u8be5\u6751\u513f\u7ae5\u8fdb\u884c\u4f5c\u4e1a\u8f85\u5bfc\uff0c\u529f\u8bfe\u590d\u4e60\u30023. \xa0 \u73ed\u7ea7\u5b66\u672f\u79d1\u6280\u5efa\u6811\u9887\u4e30\uff0c\u5927\u4e09\u5b66\u5e74\uff0c\u83b7\u5f97\u56fd\u9645\u7ea7\u7ade\u8d5b\u83b7\u59562\u4eba\u6b21\uff0c\u56fd\u5bb6\u7ea7\u7ade\u8d5b12\u4eba\u6b21\uff0c\u7701\u90e8\u7ea7\u7ade\u8d5b\u83b7\u59566\u4eba\u6b21\uff0c\u53d1\u8868\u5b66\u672f\u6587\u7ae01\u4eba\u6b21\uff0c\u7533\u8bf7\u56fd\u5bb6\u7ea7\u5927\u521b1\u9879\uff0c\u7ed3\u98985\u9879\u3002\u5bc4\u8bed\u4e0e\u611f\u8a00\u611f\u8c22\u5168\u73ed\u540c\u5b66\u4e09\u5e74\u6765\u7684\u5171\u540c\u52aa\u529b\uff0c\u611f\u8c22\u5168\u73ed\u540c\u5b66\u4e09\u5e74\u6765\u7684\u98ce\u96e8\u540c\u821f\u3002\u795d\u613f\u6240\u6709\u540c\u5b66\u5b66\u4e1a\u6709\u6210\uff0c\u5de5\u4f5c\u987a\u5229\uff0c\u524d\u7a0b\u4f3c\u9526\uff01    14011502\u73ed\u73ed\u7ea7\u7b80\u4ecb\u8f6f\u4ef6\u4e0e\u5fae\u7535\u5b50\u5b66\u966214011502\u73ed\u5927\u5bb6\u5ead\uff0c\u753124\u4eba\u7ec4\u6210\uff0c21\u4e2a\u7537\u751f3\u4e2a\u5973\u751f\uff0c\u76ee\u524d\u515a\u5458\u67093\u4eba\uff0c\u53d1\u5c55\u5bf9\u8c61\u67093\u4eba\uff0c\u5927\u4e8c\u5b66\u5e74\u5e73\u5747\u5b66\u5206\u7ee979.73\uff0c\u5e73\u5747\u7efc\u6d4b76.86\uff0c\u5b66\u9662\u6392\u540d\u7b2c\u4e8c\uff0c\u83b7\u5f97\u4f18\u79c0\u5bbf\u820d7\u4eba\u6b21\uff0c\u6821\u6570\u6a21\u7ade\u8d5b12\u4eba\u83b7\u5956\uff0c6\u4eba\u53c2\u52a0\u5927\u521b\u9879\u76ee\uff0c10\u4eba\u901a\u8fc7\u82f1\u8bed\u516d\u7ea7\u3002\u6211\u4eec\u73ed\u79ef\u6781\u53c2\u52a0\u96c6\u4f53\u6d3b\u52a8\uff0c\u6bd4\u5982\u5c0f\u73ed\u7bee\u7403\u8d5b\uff0c\u8db3\u7403\u8d5b\uff0c\u722c\u5c71\uff0c\u653e\u98ce\u7b5d\uff0c\u9a91\u884c\uff0c\u8dd1\u9a6c\u62c9\u677e\uff0c\u53c2\u52a0\u8fd0\u52a8\u4f1a\uff0c\u5728\u5b66\u4e60\u4e4b\u4f59\u653e\u677e\u8eab\u5fc3\uff0c\u953b\u70bc\u8eab\u4f53\uff0c\u540c\u65f6\u4e5f\u63d0\u9ad8\u73ed\u7ea7\u51dd\u805a\u529b\u3002\u6211\u4eec\u73ed\u7684\u5b66\u4e60\u6c1b\u56f4\u6d53\u539a\uff0c\u73ed\u7ea7\u5f62\u6210\u5b66\u4e60\u5c0f\u7ec4\uff0c\u73ed\u4e0a\u6210\u7ee9\u597d\u7684\u540c\u5b66\u5e26\u9886\u540c\u5b66\u4eec\u4e00\u8d77\u590d\u4e60\uff0c\u68b3\u7406\u77e5\u8bc6\u70b9\uff1b\u8003\u8bd5\u4e4b\u524d\uff0c\u628a\u5373\u5c06\u8003\u8bd5\u7684\u79d1\u76ee\u7684\u590d\u4e60\u8d44\u6599\u548c\u590d\u4e60\u9898\u53d1\u5230\u73ed\u7ea7\u7fa4\u91cc\uff0c\u5b66\u4e60\u59d4\u5458\u7ed9\u73ed\u4e0a\u6bcf\u4e2a\u540c\u5b66\u6253\u5370\u590d\u4e60\u9898\uff1b\u5e73\u65f6\u7684\u4f5c\u4e1a\u548c\u5b66\u4e60\u8bfe\u4ef6\u90fd\u4f1a\u5171\u4eab\u5230\u73ed\u7ea7\u7fa4\u91cc\uff0c\u5927\u5bb6\u76f8\u4e92\u5b66\u4e60\uff0c\u5171\u540c\u8fdb\u6b65\uff01\u6211\u4eec\u73ed\u7684\u73ed\u7ea7\u5ba3\u8a00\u662f\u56e2\u7ed3\u53cb\u7231\uff0c\u52e4\u594b\u8fdb\u53d6\uff01\u5148\u8fdb\u4e8b\u8ff91. \xa0\u83b7\u5f97\u8f6f\u4ef6\u4e0e\u5fae\u7535\u5b50\u5b66\u9662\u5c0f\u73ed\u8db3\u7403\u8d5b\u4e9a\u519b\uff1b2. \xa0\u76ee\u524d\u6709\u515a\u54583\u4eba\uff0c\u53d1\u5c55\u5bf9\u8c613\u4eba\uff1b3. \xa0\u5927\u4e8c\u5b66\u5e74\u5e73\u5747\u5b66\u5206\u7ee979.73\uff0c\u5e73\u5747\u7efc\u6d4b76.86\uff0c\u5b66\u9662\u6392\u540d\u7b2c\u4e8c\uff1b4. \xa0\u83b7\u5f97\u4f18\u79c0\u5bbf\u820d7\u4eba\u6b21\uff0c\u6821\u6570\u6a21\u7ade\u8d5b12\u4eba\u83b7\u5956\uff0c6\u4eba\u53c2\u52a0\u5927\u521b\u9879\u76ee\uff0c10\u4eba\u901a\u8fc7\u82f1\u8bed\u516d\u7ea7\u3002\u611f\u8a00\u4e0e\u5bc4\u8bed\u611f\u8c22\u5b66\u6821\u5bf9\u6211\u4eec\u7684\u9f13\u52b1\u4e0e\u80af\u5b9a\uff0c\u4e5f\u611f\u8c2214011502\u73ed\u6bcf\u4e2a\u6210\u5458\u7684\u52aa\u529b\uff0c\u8fd9\u4efd\u8363\u8a89\u89c1\u8bc1\u4e86\u6211\u4eec\u7684\u6210\u957f\u4e0e\u8fdb\u6b65\uff0c\u8fd9\u662f\u4e00\u4e2a\u671d\u6c14\u84ec\u52c3\uff0c\u594b\u53d1\u5411\u4e0a\uff0c\u56e2\u7ed3\u4e92\u52a9\u7684\u96c6\u4f53\uff0c\u5e0c\u671b\u6211\u4eec\u66f4\u52a0\u52aa\u529b\uff0c\u811a\u8e0f\u5b9e\u5730\uff0c\u518d\u521b\u8f89\u714c\uff01    14011605\u73ed\u73ed\u7ea7\u7b80\u4ecb\u6210\u7acb\u4e8e2016\u5e749\u6708\u7684\u8f6f\u4ef6\u5de5\u7a0b\u4e94\u73ed\u6709\u6210\u545827\u4eba\u3002\u4e0a\u5b66\u5e74\u5728\u8f85\u5bfc\u5458\uff0c\u73ed\u4e3b\u4efb\uff0c\u5404\u73ed\u59d4\u7684\u5171\u540c\u52aa\u529b\u4e0b\u5e73\u5747\u5b66\u5206\u79ef\u8fbe\u5230\u4e8676.93\u3002\u5b66\u4e60\u65b9\u9762\u4e00\u6279\u6210\u7ee9\u4f18\u5f02\u7684\u540c\u5b66\u4e50\u4e8e\u52a9\u4eba\u5e2e\u52a9\u5176\u4ed6\u540c\u5b66\u7406\u89e3\u8bfe\u7a0b\uff0c\u5171\u540c\u8fdb\u6b65\uff0c\u8003\u524d\u8fd8\u4f1a\u7ec4\u7ec7\u8003\u524d\u5b66\u4e60\u3002\u73ed\u7ea7\u5f00\u5c55\u96c6\u4f53\u6d3b\u52a8\uff0c\u5305\u62ec\u7ec4\u7ec7\u805a\u9910\uff0c\u7ec4\u7ec7\u5b66\u4e60\u515a\u5b66\u4e60\u7cbe\u795e\uff0c\u7ec4\u7ec7\u5fd7\u613f\u6d3b\u52a8\uff0c\u4e3a\u548c\u8c10\u878d\u6d3d\u7684\u73ed\u7ea7\u6c1b\u56f4\u5960\u5b9a\u4e86\u826f\u597d\u7684\u57fa\u7840\u3002\u73ed\u91cc\u540c\u5b66\u79ef\u6781\u53c2\u4e0e\u5b66\u6821\u7684\u6821\u7ea7\u6d3b\u52a8\uff0c\u4e5f\u6709\u4e0d\u5c11\u4eba\u5728\u5404\u5927\u7ade\u8d5b\u4e0a\u7a81\u7834\u81ea\u6211\uff0c\u4e5f\u53d6\u5f97\u4e86\u77a9\u76ee\u7684\u6210\u7ee9\u3002\u4e3a\u4e86\u5404\u4e2a\u5bdd\u5ba4\u7684\u536b\u751f\u7684\u751f\u6d3b\u73af\u5883\uff0c\u73ed\u91cc\u5b9a\u671f\u7ec4\u7ec7\u5c0f\u67e5\u5bdd\uff0c\u68c0\u67e5\u8fdd\u89c4\u7528\u54c1\u548c\u536b\u751f\u72b6\u51b5\uff0c\u5927\u5bb6\u5171\u540c\u521b\u9020\u826f\u597d\u7684\u5bb6\u7684\u6c1b\u56f4\u3002\u5148\u8fdb\u4e8b\u8ff91. \xa0\u5c0f\u73ed\u7bee\u7403\u8d5b\u4e9a\u519b\u3002\u5bc4\u8bed\u4e0e\u611f\u8a00\u4e00\u5e74\u7684\u73ed\u7ea7\u5efa\u8bbe\u4e2d\u6211\u4f53\u4f1a\u5230\u4e86\u91cd\u8981\u7684\u662f\u96c6\u4f53\u7684\u8fdb\u6b65\uff0c\u4e00\u4e2a\u96c6\u4f53\u5982\u679c\u80fd\u9f50\u5fc3\u534f\u529b\u4e00\u5757\u524d\u8fdb\uff0c\u6bcf\u4e2a\u4eba\u5c31\u90fd\u80fd\u591f\u83b7\u5f97\u8fdb\u6b65\u3002\u65b0\u7684\u5b66\u5e74\uff0c\u73ed\u7ea7\u7684\u5404\u79cd\u521b\u65b0\u6c1b\u56f4\u5e0c\u671b\u80fd\u591f\u66f4\u52a0\u7a81\u51fa\uff0c\u5e0c\u671b\u6bcf\u4e2a\u4eba\u90fd\u80fd\u591f\u6562\u4e8e\u7a81\u7834\u81ea\u5df1\uff0c\u53c2\u52a0\u6bd4\u8d5b\uff0c\u66f4\u5e0c\u671b\u5927\u5bb6\u8eab\u4f53\u5065\u5eb7\u3002 14011609\u73ed\u73ed\u7ea7\u7b80\u4ecb\u8fd9\u662f\u4e00\u4e2a\u671d\u6c14\u84ec\u52c3\u3001\u56e2\u7ed3\u594b\u8fdb\u3001\u6e29\u6696\u548c\u8c10\u7684\u73ed\u96c6\u4f53\uff0c26\u4f4d\u6765\u81ea\u4e94\u6e56\u56db\u6d77\u7684\u5b66\u5b50\u6c47\u96c6\u5728\u8fd9\u91cc\uff0c\u4e3a\u4e86\u5171\u540c\u7684\u68a6\u60f3\u800c\u594b\u6597\u3002\u6211\u4eec\u5171\u5c5e\u4e8e1401109\u73ed\u8fd9\u4e2a\u96c6\u4f53\uff0c\u4e00\u5e74\u4ee5\u6765\uff0c\u5728\u8f85\u5bfc\u5458\u8001\u5e08\u7684\u5173\u6000\u548c\u73ed\u59d4\u7684\u9886\u5bfc\u4e0b\uff0c\u5168\u73ed\u540c\u5b66\u79ef\u6781\u8fdb\u53d6\u3001\u56e2\u7ed3\u4e00\u5fc3\uff0c\u53d6\u5f97\u4e86\u4f18\u5f02\u7684\u6210\u7ee9\u3002\u5728\u73ed\u7ea7\u5efa\u8bbe\u4e0a\uff0c\u6211\u4eec\u4e89\u521b\u4e00\u6d41\uff0c\u73ed\u59d4\u6210\u5458\u8ba4\u771f\u8d1f\u8d23\uff0c\u591a\u6b21\u4e3e\u884c\u73ed\u4f1a\u3001\u5b66\u4e60\u5206\u4eab\u4ea4\u6d41\u4f1a\uff0c\u8ba4\u771f\u5b8c\u6210\u5e03\u7f6e\u7684\u5404\u9879\u5de5\u4f5c\uff0c\u5e76\u7ec4\u7ec7\u5f00\u5c55\u56e2\u65e5\u6d3b\u52a8\u3002\u5728\u5b66\u4e60\u4e0a\uff0c\u73ed\u7ea7\u540c\u5b66\u52aa\u529b\u5b66\u4e60\uff0c\u540c\u65f6\u6211\u4eec\u7ec4\u7ec7\u4f18\u79c0\u5b66\u751f\u5f00\u5c55\u5b66\u4e60\u4ea4\u6d41\u6d3b\u52a8\uff0c\u4e3a\u5927\u5bb6\u5206\u4eab\u7ecf\u9a8c\uff0c\u6700\u7ec8\u53d6\u5f97\u4e86\u4e0a\u4e00\u5b66\u5e74\u5e73\u5747\u5b66\u5206\u79ef\u4e13\u4e1a\u7b2c\u4e00\u7684\u597d\u6210\u7ee9\uff0c\u53e6\u6709\u591a\u4eba\u6210\u7ee9\u540d\u5217\u524d\u8305\uff1b\u5728\u7ade\u8d5b\u65b9\u9762\uff0c\u591a\u4f4d\u540c\u5b66\u53c2\u52a0\u5404\u9879\u79d1\u6280\u7c7b\u6bd4\u8d5b\u548c\u79d1\u521b\u57fa\u5730\uff0c\u53d6\u5f97\u4e86\u4f18\u5f02\u7684\u6210\u7ee9\uff0c\u4e3a\u5176\u4ed6\u540c\u5b66\u6811\u7acb\u4e86\u699c\u6837\u3002\u73ed\u7ea7\u5171\u670923\u540d\u7537\u751f\uff0c3\u540d\u5973\u751f\u3002\u8fd9\u91cc\u6709\u8bfe\u7a0b\u5b66\u9738\u3001\u7ade\u8d5b\u8fbe\u4eba\uff0c\u4e5f\u6709\u591a\u624d\u591a\u827a\u3001\u80fd\u6b4c\u5584\u821e\u7684\u540c\u5b66\uff0c\u8fd8\u6709\u591a\u4f4d\u5b66\u751f\u4f1a\u3001\u793e\u56e2\u7b49\u5b66\u751f\u7ec4\u7ec7\u7684\u9aa8\u5e72\u6210\u5458\u3002\u6211\u4eec\u5c31\u662f\u8fd9\u6837\u4e00\u4e2a\u4f18\u79c0\u7684\u96c6\u4f53\uff0c\u6211\u4eec\u575a\u4fe1\uff0c\u5728\u6211\u4eec\u7684\u5171\u540c\u52aa\u529b\u4e0b\uff0c14011609\u73ed\u5c06\u521b\u9020\u4e00\u4e2a\u66f4\u52a0\u8f89\u714c\u7684\u672a\u6765\uff01\u5148\u8fdb\u4e8b\u8ff91.\xa0 \u73ed\u7ea7\u5e73\u5747\u5b66\u5206\u79ef\u4e13\u4e1a\u7b2c\u4e00\uff0c\u5e74\u7ea7\u7b2c\u4e8c\uff1b2.\xa0 \u4fde\u5e06\u3001\u51af\u52c7\u8d85\u5b66\u5206\u79ef\u6392\u540d\u4f4d\u4e8e\u4e13\u4e1a\u524d\u5341\u540d\uff1b3. \xa0\u674e\u6cf3\u6c50\u3001\u8c22\u542f\u806a\u7efc\u5408\u6d4b\u8bc4\u6392\u540d\u4f4d\u4e8e\u4e13\u4e1a\u524d\u5341\u540d\uff1b4.\xa0 \u5168\u73ed\u5171\u83b7\u5f97\u56fd\u9645\u59561\u9879\uff0c\u56fd\u5bb6\u7ea7\u5956\u98792\u9879\uff0c\u7701\u90e8\u7ea7\u5956\u98793\u9879\uff0c\u6821\u595612\u9879\uff0c\u5927\u521b\u9879\u76ee4\u9879\uff1b5.\xa0 \u73ed\u7ea7\u5171\u67094\u4eba\u88ab\u8bc4\u4e3a\u4f18\u79c0\u5171\u9752\u56e2\u5458\uff1b6.\xa0 \u8c22\u542f\u806a\u83b7\u5f97\u56fd\u5bb6\u5956\u5b66\u91d1\uff0c\u674e\u6cf3\u6c50\u83b7\u5f97\u7cbe\u96d5\u7279\u7b49\u5956\u5b66\u91d1\uff0c\u51af\u52c7\u8d85\u83b7\u5f97\u7cbe\u96d5\u4e8c\u7b49\u5956\u5b66\u91d1\uff0c\u4fde\u5e06\u83b7\u5f97\u79d1\u4e3a\u5956\u5b66\u91d1\uff1b7.\xa0 \u73ed\u7ea7\u5171\u67094\u4eba\u83b7\u5f97\u6821\u4f18\u79c0\u5b66\u751f\uff0c8\u4eba\u83b7\u5f97\u9662\u4f18\u79c0\u5b66\u751f\u3002\u5bc4\u8bed\u4e0e\u611f\u8a00\u80fd\u5f97\u5230\u201c\u5148\u8fdb\u73ed\u7ea7\u201d\u7684\u8363\u8a89\uff0c\u6211\u4eec\u611f\u5230\u5341\u5206\u5f00\u5fc3\u4e0e\u6fc0\u52a8\uff0c\u8fd9\u662f\u5b66\u9662\u5bf9\u6211\u4eec\u73ed\u7ea7\u5de5\u4f5c\u7684\u80af\u5b9a\u4e0e\u9f13\u52b1\uff0c\u4e5f\u662f\u5bf9\u6211\u4eec\u6210\u679c\u4ed8\u51fa\u7684\u8ba4\u53ef\u3002\u611f\u8c22\u8f85\u5bfc\u5458\u8001\u5e08\u5bf9\u6211\u4eec\u73ed\u7ea7\u5de5\u4f5c\u7684\u6307\u5bfc\u4e0e\u5e2e\u52a9\uff0c\u4e5f\u611f\u8c22\u73ed\u4e3b\u4efb\u8001\u5e08\u5bf9\u6211\u4eec\u5b66\u4e60\u4e0a\u7684\u6307\u5bfc\uff0c\u5f53\u7136\u8fd9\u4e5f\u79bb\u4e0d\u5f00\u73ed\u7ea7\u540c\u5b66\u5bf9\u73ed\u96c6\u4f53\u7684\u4ed8\u51fa\u4e0e\u8ba4\u540c\u3002\u6211\u4eec\u5e76\u4e0d\u6ee1\u8db3\u4e8e\u6b6