<?xml version="1.0" encoding="UTF-8"?>
<webpage>
	<!--修改信息表单，需要提交的内容-->
	<person>
		<crsf>{% csrf_token %}</crsf>
		<action>{% url 'person_edit' %}</action>
		<sex>{{ form.sex }}</sex>
		<age>{{ form.age }}</age>
		<phone>{{ form.phone}}</phone>
		<desc>{{ form.desc}}</desc>
		<img>{{ form.img}}</img>
	</person>
</webpage>